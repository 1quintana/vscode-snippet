{
    "name": "Snippet",
    "version": "0.0.3",
    "publisher": "vscode-snippet",
    "engines": {
        "vscode": "^1.19.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mre/vscode-snippet.git"
    },
    "license": "MIT",
    "displayName": "Snippet",
    "description": "Insert a snippet from cht.sh",
    "icon": "contrib/icon.png",
    "categories": [
        "Snippets",
        "Other"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "title": "Find",
                "command": "snippet.find",
                "category": "Snippet"
            }
        ],
        "configuration": {
            "title": "Snippet Configuration",
            "properties": {}
        }
    },
    "activationEvents": [
        "onCommand:snippet.find"
    ],
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    }
}